{
    "name": "armrest/client",
    "type": "library",
    "description": "Armrest PHP Client Library",
    "keywords": ["rest", "api"],
    "homepage": "http://github.com/payload-code/armrest-php",
    "license": "MIT",
    "authors": [{
        "name": "Payload",
        "email": "help@payload.co"
    }],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "0.3.5",
    "autoload": {
        "psr-4": {
            "Armrest\\": "Armrest/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpcompatibility/php-compatibility": "*",
        "squizlabs/php_codesniffer": "3.*"
    }
}
